{
    "name": "attla/flash-messages",
    "description": "Intuitive package to flash notifications on laravel.",
    "keywords": [
        "attla",
        "flash",
        "flash-message",
        "flash-notification",
        "laravel-flash",
        "laravel-flash-message",
        "laravel-flash-notification"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Nicolau",
            "email": "lucas@octha.com"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "attla/support": "dev-main",
        "illuminate/session": "^6.0|^7.0|^8.0|^9.0"
    },
    "conflict": {
        "laracasts/flash": ">=0.0.1",
        "spatie/laravel-flash": ">=0.0.1"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Attla\\Flash\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Attla\\Flash\\ServiceProvider"
            ],
            "aliases": {
                "Flash": "Attla\\Flash\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
